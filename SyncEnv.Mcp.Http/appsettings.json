{
  "SyncConfiguration": {
    "Environments": {
      "Local": {
        "ConnectionString": "in-memory://local",
        "DatabaseName": "local_inmemory"
      },
      "Production": {
        "ConnectionString": "in-memory://production",
        "DatabaseName": "production_inmemory"
      }
    },
    "Collections": {
      "Games": "games",
      "Teams": "teams", 
      "Competitions": "competitions",
      "Seasons": "seasons",
      "Players": "players",
      "Events": "events",
      "Statistics": "statistics",
      "LineUps": "lineups"
    },
    "SyncOptions": {
      "BatchSize": 100,
      "MaxConcurrency": 5,
      "TimeoutSeconds": 30,
      "RetryAttempts": 3
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "SyncEnv.Mcp.Http": "Information"
    }
  }
}